{
  "name": "mobile",
  "version": "0.11.17",
  "private": true,
  "workspaces": {
    "nohoist": [
      "mobx-react-lite"
    ]
  },
  "scripts": {
    "clean": "sh ./scripts/clean.sh",
    "android": "yarn build:provider && react-native run-android",
    "ios": "yarn build:provider && react-native run-ios",
    "android:connect": "adb reverse tcp:8081 tcp:8081",
    "typecheck": "tsc",
    "build:provider": "tsc --project tsconfig.provider.json && browserify -p tinyify build/injected/index.js -o build/injected/injected-provider.bundle.js && cp -rf build/injected/injected-provider.bundle.js android/app/src/main/assets/.",
    "start": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "generate:splash-image": "expo-splash-screen --background-color \"#FBF8FF\" --image-path ./src/assets/logo/splash-screen-only-k.png --image-resize-mode contain --status-bar-translucent --status-bar-background-color \"#FBF8FF\" --status-bar-style dark-content",
    "pod-install": "pod-install",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "sh ./scripts/fix-android-splash-screen-flash.sh && sh ./scripts/fix-ios-blur-view.sh && sh ./scripts/force-export-ble-manager-from-ledger.sh"
  },
